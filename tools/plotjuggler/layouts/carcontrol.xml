  <snippet name="calcDesiredSpeed">
   <global>prevX = 0
calcDesiredSpeed = 0
is_first = true</global>
   <function>if (is_first) then
  is_first = false
  prevX = time
  calcDesiredSpeed = v1
end

dx = time - prevX
prevX = time

if (math.abs(value) &lt; 0.1) then
  calcDesiredSpeed = v1
end

calcDesiredSpeed = calcDesiredSpeed + value*dx

return calcDesiredSpeed</function>
